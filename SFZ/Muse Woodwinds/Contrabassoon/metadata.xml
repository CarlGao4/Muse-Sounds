<?xml version="1.0" encoding="utf-8" standalone="no"?>
<Instrument Vendor="Muse" Pack="Muse Woodwinds" Category="Muse Woodwinds" BracketGroup="Woodwinds" Volume="-4" Reverb="50" DefaultClef="BASS" FriendlyName="Contrabassoon" Abbreviation="Cbsn.">
  <Transposition ImplicitOctaves="-1" TransposeOctaves="-1"/>
