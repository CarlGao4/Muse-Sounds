  <Articulation File="SFZ\Contrabassoon - Mordent Inverted Minor.sfz" Symbol="MordentInvertedSemi" OffsetStart="30"/>
  <Articulation File="SFZ\Contrabassoon - Turn Major.sfz" Symbol="TurnWholeSemi" OffsetStart="30"/>
  <Articulation File="SFZ\Contrabassoon - Turn Minor.sfz" Symbol="TurnSemiSemi" OffsetStart="30"/>
  <Articulation File="SFZ\Contrabassoon - Turn Inverted Major.sfz" Symbol="TurnInvertedWholeSemi" OffsetStart="30"/>
  <Articulation File="SFZ\Contrabassoon - Turn Inverted Minor.sfz" Symbol="TurnInvertedSemiSemi" OffsetStart="30"/>
  <Articulation File="SFZ\Contrabassoon - Fall.sfz" Symbol="Fall" OffsetStart="30" SetDuration="75"/>
  <Articulation File="SFZ\Contrabassoon - Effects.sfz" Symbol="Text" OffsetStart="30" Name="flaps|key clicks|effects"/>
  <!-- Realtime -->
  <Articulation File="SFZ\Contrabassoon - Realtime.sfz" Symbol="Realtime"/>
  <MusicXML InstrumentSound="wind.reed.contrabassoon"/>
  <MPE InstrumentSound="winds.contrabassoon"/>
</Instrument>
