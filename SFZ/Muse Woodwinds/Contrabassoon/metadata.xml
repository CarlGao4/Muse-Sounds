  <Articulation File="SFZ\Contrabassoon - Trill Whole.sfz" Symbol="Trill" ForInterval="2" OffsetStart="30"/>
  <!-- Quick repetitions -->
  <Articulation File="SFZ\Contrabassoon - Staccato.sfz" ForRepetition="|150ms" OffsetStart="30"/>
  <Articulation File="SFZ\Contrabassoon - Staccato.sfz" Symbol="Legato" ForRepetition="|150ms" OffsetStart="30"/>
  <!-- Short Notes -->
  <Articulation File="SFZ\Contrabassoon - Staccato.sfz" Class="Staccato" OffsetStart="30"/>
  <Articulation File="SFZ\Contrabassoon - Staccato Accent.sfz" Symbol="Staccato|Accent" OffsetStart="30"/>
  <Articulation File="SFZ\Contrabassoon - Staccatissimo.sfz" Class="Staccato_Fastest" OffsetStart="30"/>
  <Articulation File="SFZ\Contrabassoon - Staccatissimo.sfz" Class="Staccatissimo" OffsetStart="30"/>
  <!-- Ornaments -->
  <Articulation File="SFZ\Contrabassoon - Mordent Major.sfz" Symbol="MordentWhole" OffsetStart="30"/>
  <Articulation File="SFZ\Contrabassoon - Mordent Minor.sfz" Symbol="MordentSemi" OffsetStart="30"/>
