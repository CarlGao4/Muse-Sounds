  <SamplerEffect>
    <Processor Name="ParametricEqBand">
      <Param Name="Q" Value="1"/>
      <Param Name="Freq" Value="400"/>
      <Param Name="Gain" Value="-9"/>
    </Processor>
  </SamplerEffect>
  <!-- Long Notes -->
  <Articulation File="SFZ\Contrabassoon - Legato Control.sfz" Class="Sustain"/>
