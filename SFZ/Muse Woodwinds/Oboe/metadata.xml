  <Articulation File="SFZ\Oboe - Trill Whole.sfz" Symbol="Trill" ForInterval="2" OffsetStart="30"/>
  <!-- Quick repetitions -->
  <Articulation File="SFZ\Oboe - Staccato.sfz" ForRepetition="|140ms" OffsetStart="30"/>
  <Articulation File="SFZ\Oboe - Staccato.sfz" Symbol="Legato" ForRepetition="|140ms" OffsetStart="30"/>
  <!-- Short Notes -->
  <Articulation File="SFZ\Oboe - Staccato.sfz" Class="Staccato" OffsetStart="30"/>
  <Articulation File="SFZ\Oboe - Staccatissimo.sfz" Class="Staccato_Fastest" OffsetStart="30"/>
  <Articulation File="SFZ\Oboe - Staccatissimo.sfz" Class="Staccatissimo" OffsetStart="30"/>
