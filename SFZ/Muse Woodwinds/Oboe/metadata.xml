  <Articulation File="SFZ\Oboe - Mordent Major.sfz" Symbol="MordentWhole" OffsetStart="30"/>
  <Articulation File="SFZ\Oboe - Mordent Minor.sfz" Symbol="MordentSemi" OffsetStart="30"/>
