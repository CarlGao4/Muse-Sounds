  <SamplerEffect>
    <Processor Name="ParametricEqBand">
      <Param Name="Q" Value="0.8"/>
      <Param Name="Freq" Value="1500"/>
      <Param Name="Gain" Value="-4"/>
    </Processor>
    <Processor Name="ParametricEqBand">
      <Param Name="Q" Value="2"/>
      <Param Name="Freq" Value="400"/>
      <Param Name="Gain" Value="-2"/>
    </Processor>
  </SamplerEffect>
  <!-- Long Notes -->
  <Articulation File="SFZ\Oboe - Legato Control.sfz" Class="Sustain"/>
