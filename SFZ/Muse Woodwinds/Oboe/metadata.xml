<?xml version="1.0" encoding="utf-8" standalone="no"?>
<Instrument Vendor="Muse" Pack="Muse Woodwinds" Category="Muse Woodwinds" BracketGroup="Woodwinds" Name="Oboe" Volume="-12" Reverb="50" FriendlyName="Oboe" Abbreviation="Ob." InsertableSymbols="OpenHarmonic|Mute">
