  <Articulation File="SFZ\Oboe - Mordent Inverted Minor.sfz" Symbol="MordentInvertedSemi" OffsetStart="30"/>
  <Articulation File="SFZ\Oboe - Turn Major.sfz" Symbol="TurnWholeSemi" OffsetStart="30"/>
  <Articulation File="SFZ\Oboe - Turn Minor.sfz" Symbol="TurnSemiSemi" OffsetStart="30"/>
  <Articulation File="SFZ\Oboe - Turn Inverted Major.sfz" Symbol="TurnInvertedWholeSemi" OffsetStart="30"/>
  <Articulation File="SFZ\Oboe - Turn Inverted Minor.sfz" Symbol="TurnInvertedSemiSemi" OffsetStart="30"/>
